{
  "name": "gsg-badge",
  "version": "0.0.3",
  "description": "Get-Set-Go badge component",
  "main": "dist/index.min.js",
  "scripts": {
    "dist:clean": "rimraf dist",
    "eg:clean": "rimraf examples/dist",
    "test:clean": "rimraf jest",
    "coverage:clean": "rimraf coverage",
    "bundle": "npm run dist:clean && webpack --progress -d --watch",
    "bundle:eg": "npm run eg:clean && webpack --config \"./examples/webpack.config.js\" --progress -p -d --watch",
    "build": "npm run dist:clean && cross-env NODE_ENV=production webpack --progress -p -d",
    "build:eg": "npm run eg:clean && cross-env NODE_ENV=production webpack --config \"./examples/webpack.config.js\" --progress -p -d",
    "build:prod": "npm run build && npm run build:eg",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "npm run test:clean && npm run coverage:clean && jest",
    "postinstall": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/get-set-go/gsg-badge.git"
  },
  "keywords": [
    "Get-Set-Go",
    "React",
    "Components",
    "Bootstrap",
    "badge"
  ],
  "author": "Ashwin Hegde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/get-set-go/gsg-badge/issues"
  },
  "homepage": "https://github.com/get-set-go/gsg-badge#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "object-assign": "^4.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "gsg-common-style": "https://github.com/get-set-go/gsg-common-style.git"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "eslint": "^3.11.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3",
    "jest-cli": "^17.0.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "pre-commit": [
    "lint",
    "lint:fix",
    "test"
  ],
  "jest": {
    "transform": {
      ".*": "./jest.preprocessor.conf"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom"
    ]
  }
}
